/* component/listcom/listcom.wxss */
.note-item-c {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  padding: -webkit-box;
  position: relative;
  line-height: 1.3;
  color: #666;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 20rpx;
  text-align: left;
  height: 40px;
}

.note-item-t {
  text-align: right;
  padding-right: 20rpx;
  color: #666;
  line-height: 2;
  border-bottom: 1px solid #ccc;
}