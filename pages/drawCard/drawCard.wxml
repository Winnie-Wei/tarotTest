<view class="card-view" wx:for="{{imgList}}" wx:key="{{index}}" bindtouchmove="drag" >
  <image id='{{item.id}}' class="imgcss t back" src='{{item.src}}' style="left:{{item.left}}px;top:{{item.top}}px;z-index:{{item.zindexB}};transform:{{item.transB}};" ></image>
  <image id='{{item.id + "back"}}' class="imgcss t font" src='../../image/waiteTarot/78.jpg' style="left:{{item.left}}px;top:{{item.top}}px;z-index:{{item.zindexF}};transform:{{item.transF}};" bindtap="turnback"></image>
</view>



<button class='share' type='primary' bindtap='share'>生成分享图</button>
<button type='primary' size='mini' bindtap='save'>保存分享图</button>