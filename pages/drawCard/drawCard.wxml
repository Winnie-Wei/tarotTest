<swiper>
  <swiper-item>
    <view class="card-view" wx:for="{{imgList}}" wx:key="{{index}}" bindtouchmove="drag">
      <view style='color:#fff;position:absolute;left:{{item.left}}px;top:{{item.top}}px;z-index:3;'>{{index+1}}0</view>
      <image id='{{item.id}}' class="imgcss t back" src='{{item.src}}' style="left:{{item.left}}px;top:{{item.top}}px;z-index:{{item.zindexB}};transform:{{item.transB}};"></image>
